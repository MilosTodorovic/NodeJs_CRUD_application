<%- include('header') %>

    <form method="post" action="/editDocPartById/<%= document_part.DOC_ID %>">
        <div class="form-group">
            <label for="docId">Document part ID</label>
            <input type="text" class="form-control" id="docId" name="docId" value="<%=  document_part.DOC_ID %>">
        </div>
        <div class="form-group">
            <label for="docName">Document part name</label>
            <input type="text" class="form-control" id="docName" name="docName" value="<%=  document_part.DOC_PARTH_NAME %>">
        </div><br>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>

<%- include('footer') %>


   